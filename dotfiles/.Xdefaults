!AMBIANCE
Xft*dpi:              96
Xft*antialias:        True
Xft*hinting:          Full

URxvt.scrollBar:         false
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true
URxvt.transparent: true
URxvt.shading:     10

!urxvt.font:     xft:DejaVu Sans Mono for Powerline:pixelsize=12
!URxvt*font:             xft:Bitstream Vera Sans Mono for Powerline:pixelsize=14:antialias=true,\
!URxvt*font:                        xft:Liberation Mono for Powerline:pixelsize=12:antialias=true,\
URxvt*font:                        xft:Monospace:pixelsize=12.5:antialias=true,\
                        xft:Inconsolata\ for\ Powerline:pixelsize=12,\
                        xft:PowerlineSymbols:pixelsize=12.5:antialias=true
!URxvt*boldfont:                        xft:Liberation Mono for Powerline:style=bold:pixelsize=12:antialias=true,\
URxvt*boldfont:                        xft:Monospace:style=bold:pixelsize=12.5:antialias=true,\
i                       xft:Inconsolata\ for\ Powerline:pixelsize=12,\
                        xft:PowerlineSymbols:style:bold:pixelsize=12.5:antialias=true

!URxvt*font:               xft:Inconsolata:style=Semibold:pixelsize=14:antialias=true:hinting=slight
!URxvt*boldFont:           xft:Inconsolata:style=bold:pixelsize=14:antialias=true:hinting=slight

URxvt.foreground: #C8C8C8
URxvt.background: #323232
URxvt.color0:     #2E3436
URxvt.color1:     #CC0000
URxvt.color2:     #4E9A06
URxvt.color3:     #C4A000
URxvt.color4:     #3465A4
URxvt.color5:     #75507B
URxvt.color6:     #06989A
URxvt.color7:     #D3D7CF
URxvt.color8:     #555753
URxvt.color9:     #EF2929
URxvt.color10:    #8AE234
URxvt.color11:    #FCE94F
URxvt.color12:    #729FCF
URxvt.color13:    #AD7FA8
URxvt.color14:    #34E2E2
URxvt.color15:    #EEEEEC
